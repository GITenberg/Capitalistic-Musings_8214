\par }\pard \ltrpar\ql \li0\ri0\sb100\sa100\sbauto1\saauto1\widctlpar\wrapdefault\aspalpha\aspnum\faauto\adjustright\rin0\lin0\itap0\pararsid337851 {\rtlch \af0\afs24 \ltrch \f0\fs24\lang1033\langfe1033\langnp1033\insrsid337851 
